{
    "dev": {
        "app_function": "leerPagina.obtener_html",
        "keep_warm": false,
        "role_name": "LabRole",
        "manage_roles": false,
        "apigateway_enabled": false,
        "aws_region": "us-east-1",
        "project_name": "parcial1_lambda1",
        "runtime": "python3.8",
        "s3_bucket": "parcial1bigdata",
        "events": [{
           "function": "leerPagina.obtener_html", 
           "expression": "cron(38 19 * * ? *)" 
       }]
    },
    
    "dev2": {
        "app_function": "recibirPagina.pagina",
        "keep_warm": false,
        "role_name": "LabRole",
        "manage_roles": false,
        "apigateway_enabled": false,
        "aws_region": "us-east-1",
        "project_name": "parcial1_lambda2",
        "runtime": "python3.8",
        "s3_bucket": "parcial1bigdata",
        "events": [{
            "function": "recibirPagina.pagina",
            "event_source": {
                  "arn":  "arn:aws:s3:::parcial1bigdata",
                  "events": [
                    "s3:ObjectCreated:*" 
                  ]
               }
            }]
    }
    
}
